{
  "name": "@m6web/i18n-translate",
  "version": "1.0.0-alpha.0",
  "description": "Translation and pluralization functions for m6web-i18n",
  "keywords": [
    "i18n",
    "translation"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": "m6web",
  "license": "MIT",
  "files": [
    "lib/",
    "es/"
  ],
  "homepage": "https://github.com/M6Web/i18n-tools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M6Web/i18n-tools.git"
  },
  "dependencies": {
    "lodash": "4.17.11",
    "sprintf-js": "1.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-use-lodash-es": "^0.2.0",
    "babel-preset-env": "1.7.0",
    "eslint-plugin-jest": "24.0.2",
    "jest": "26.6.3",
    "lodash-es": "4.17.11"
  },
  "bugs": {
    "url": "https://github.com/M6Web/i18n-tools/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "babel-watch": "babel src --out-dir lib --watch --ignore __tests__,__mocks__",
    "build": "yarn build:commonjs && yarn build:es",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib --ignore __tests__,__mocks__",
    "build:es": "BABEL_ENV=es babel src --out-dir es --ignore __tests__,__mocks__",
    "format": "prettier-eslint --write src/**/*.js *.md",
    "prepack": "yarn test && yarn build",
    "test": "BABEL_ENV=test jest",
    "release": "yarn lint && yarn test && yarn build && yarn version"
  }
}
